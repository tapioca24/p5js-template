{
  "name": "p5js-template",
  "version": "1.0.0",
  "description": "Template for p5.js sketches using VS Code, Prettier and ESLint.",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write --ignore-path config/.prettierignore .",
    "lint": "eslint --config config/.eslintrc.js --ignore-path config/.eslintignore './**/*.{js,ts}'",
    "lint:fix": "eslint --config config/.eslintrc.js --ignore-path config/.eslintignore --fix './**/*.{js,ts}'",
    "new": "hygen sketch new"
  },
  "devDependencies": {
    "@fal-works/eslint-config-p5js": "^1.0.3",
    "@types/p5": "^1.4.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "hygen": "^6.1.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  }
}
